{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "npm run icons && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "npm run icons && cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run icons && npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "concurrently \"npm run watch:icons\"  \"cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js\"",
    "watch-poll": "npm run watch -- --watch-poll",
    "icons": "node frontend/tasks/icons.js",
    "watch:icons": "watch 'npm run icons' frontend/icons --wait=2 --interval=0.1",
    "serve": "vue-cli-service serve frontend",
    "build": "vue-cli-service build frontend",
    "lint": "vue-cli-service lint frontend"
  },
  "dependencies": {
    "core-js": "^3.2.1",
    "axios": "^0.18.1",
    "cropperjs": "^1.4.1",
    "date-fns": "1.29.0",
    "fine-uploader": "^5.16.2",
    "fine-uploader-wrappers": "^1.1.0",
    "flatpickr": "4.3.2",
    "lodash": "^4.17.15",
    "quill": "1.3.6",
    "smartcrop": "^2.0.3",
    "tinycolor2": "1.4.1",
    "truncate-utf8-bytes": "1.0.2",
    "vue": "^2.6.10",
    "vue-select": "2.5.1",
    "vue-timeago": "^3.4.4",
    "vuedraggable": "2.16.0",
    "vuetrend": "^0.3.2",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "assets-webpack-plugin": "^3.9.10",
    "babel-eslint": "^10.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "laravel-mix": "^5.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.22.12",
    "sass-loader": "^7.3.1",
    "standard": "^11.0.1",
    "svg-spritemap-webpack-plugin": "^3.4.0",
    "vue-template-compiler": "^2.6.10",
    "watch": "1.0.2",
    "webpack-assets-manifest": "^3.1.1"
  }
}
